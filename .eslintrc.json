{
  "env": {
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "selfbot_cljs": false
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "no-else-return": "error",
    "no-console": "off",
    "no-var": "error",
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "warn",
      "double"
    ],
    "semi": [
      "warn",
      "always"
    ]
  }
}
